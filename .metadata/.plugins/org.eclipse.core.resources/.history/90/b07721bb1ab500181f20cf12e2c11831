package com.jesse.pinkman;

/**
 * @author Latheef
 *
 */
public class FactorialOfProgram {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=100,factorial = 1;
		
		for (int i = n; n > 1; i--) {
			
			factorial*=n*(n-1);
			System.out.println(factorial);
			n-=2;
		}
		System.out.println(factorial);
	}

}
