package com.data;

public class EvenOdd {

	public static void main(String[] args) {
		
		System.out.print("Even: ");
		for (int i = 1; i < 100; i++) {
			
				
				if(i%2==0)
				{
				  System.out.print(i);	
				}
					
		}
		
		for (int i = 1; i < 100; i++) {
			
			
			if(i%2!=0)
			{
			  System.out.print("Odd: "+i);	
			}
			
		
		
	}
	}
	
}
