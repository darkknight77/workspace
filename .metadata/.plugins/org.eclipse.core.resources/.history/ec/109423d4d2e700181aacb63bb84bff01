package com.data;

public class ReverseAstring {

	public static String ReverseStringUsingCharArrray(String s){
		
		char[] c=s.toCharArray();
		String rev="";
		for (int i = c.length; i >=0; i--) {
			
			rev=rev+c[i];
			
		}
		
		
		return rev;
		
	} 
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		
	}

}
