package com.data;

public class EvenOdd {

	public static void main(String[] args) {
		
		for (int i = 1; i < 100; i++) {
			
				
				if(i%2==0)
				{
				  System.out.print("Even: "+i);	
				}
					
		}
		
		for (int i = 1; i < 100; i++) {
			
			
			if(i%2==0)
			{
			  System.out.print("Even: "+i);	
			}
			
		
		
	}
	}
	
}
