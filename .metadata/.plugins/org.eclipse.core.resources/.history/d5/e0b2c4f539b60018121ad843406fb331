package com.jesse.pinkman;

public class pattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 5;

		for (int i = 0; i < 5; i++) {

			for (int j = 4; j >= n; j--) {

				System.out.print("*");
				
			}

			System.out.println();
			n--;
			
		}

	}

}
