package com.jesse.pinkman;

public class pattern7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=1;
		int x=1;
		
		for (int i = 0; i < 5; i++) {
			
			for (int j = 0; j < n; j++) {
				
				System.out.print(x);
				x++;
			}
			
			System.out.println();
			n++;
			
		}
		
		
	}

}
