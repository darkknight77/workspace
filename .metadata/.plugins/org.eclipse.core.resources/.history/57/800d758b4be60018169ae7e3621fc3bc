package com.data;

public class pattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n = 1, s = 3;
		for (int i = 0; i < 5; i++) {

			for (int j = 0; j < s; j++) {

				System.out.print(" ");
			}
		
			for (int j = 0; j < n; j++) {
			
				System.out.print("*");
			}
			
			n++;
			s--;
			System.out.println();
		}

	}

}
